<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/rl_edit_info" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:background="@drawable/upomp_bypay_bg"
	android:layout_marginBottom="35dip" android:layout_marginLeft="15dip"
	android:layout_marginRight="15dip" android:layout_marginTop="35dip">
	<LinearLayout android:focusable="true"
		android:focusableInTouchMode="true" android:layout_width="0dip"
		android:layout_height="0dip" />
	<RelativeLayout android:id="@+id/rl_header_change_tel"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/upomp_bypay_title_bg">
		<Button android:id="@+id/btn_return_change_tel"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:background="@drawable/upomp_bypay_btn_title_esc"
			android:layout_alignParentRight="true" android:layout_marginRight="5dip"
			android:layout_marginTop="5dip" android:textColor="#ffffffff"
			android:text="@string/upomp_bypay_return" />
	</RelativeLayout>



	<ScrollView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_below="@id/rl_header_change_tel"
		android:layout_above="@+id/rl_bottom_change_tel">
		<RelativeLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content">

			<ImageView android:id="@+id/iv_top1_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:background="@drawable/upomp_bypay_info_top2"
				android:layout_marginTop="20dip" android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip" />
			<RelativeLayout android:id="@+id/rl_name_old_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_below="@id/iv_top1_change_tel" android:background="@drawable/upomp_bypay_info_bg"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
				<RelativeLayout android:id="@+id/rl_iv1_change_tel"
					android:layout_width="40dip" android:layout_height="40dip"

					android:background="@drawable/upomp_bypay_info_left">
					<ImageView android:id="@+id/iv1_change_tel"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_centerVertical="true" android:background="@drawable/upomp_bypay_info_icon4" />
				</RelativeLayout>
				<TextView android:id="@+id/tv_name_title_old_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:gravity="center_vertical" android:text="@string/upomp_bypay_getpass_name"
					android:textSize="15sp" android:textColor="#6a7482"
					android:layout_marginLeft="10dip" android:layout_toRightOf="@+id/rl_iv1_change_tel"
					android:layout_centerVertical="true" />

				<TextView android:id="@+id/tv_name_content_old_change_tel"
					android:singleLine="true" android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:numeric="integer"
					android:maxLength="11" android:textColor="#6a7482"
					android:layout_centerVertical="true" android:textSize="15sp"
					android:background="#00000000" android:layout_toRightOf="@+id/tv_name_title_old_change_tel" />

			</RelativeLayout>
			<ImageView android:id="@+id/iv_bot1_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:background="@drawable/upomp_bypay_info_bot2"
				android:layout_below="@id/rl_name_old_change_tel"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip" />



			<RelativeLayout android:id="@+id/rl_login_psw_content_change_tel"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_below="@id/iv_bot1_change_tel"
				android:layout_marginTop="8dip" android:background="@drawable/upomp_bypay_input_bg"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
				<TextView android:id="@+id/tv_login_psw_content_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="@string/upomp_bypay_pswname" android:textColor="#6a7482"
					android:textSize="15sp" android:layout_centerVertical="true"
					android:layout_marginLeft="5dip" />

				<Button android:id="@+id/btn_login_psw_content_change_tel"
					android:layout_width="fill_parent" android:layout_height="45dip"
					android:hint="@string/upomp_bypay_user_psw" android:textColor="#6a7482"
					android:layout_marginLeft="5dip" android:textSize="15sp"
					android:layout_toRightOf="@+id/tv_login_psw_content_change_tel"
					android:gravity="left|center" android:background="#00000000" />

			</RelativeLayout>



			<ImageView android:id="@+id/iv_top_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:background="@drawable/upomp_bypay_info_top2"
				android:layout_marginTop="8dip" android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip" android:layout_below="@id/rl_login_psw_content_change_tel" />
			<RelativeLayout android:id="@+id/rl_telnum_old_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_below="@id/iv_top_change_tel" android:background="@drawable/upomp_bypay_info_bg"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
				<RelativeLayout android:id="@+id/rl_iv_change_tel"
					android:layout_width="40dip" android:layout_height="40dip"

					android:background="@drawable/upomp_bypay_info_left">
					<ImageView android:id="@+id/iv_change_tel"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_centerVertical="true" android:background="@drawable/upomp_bypay_info_icon5" />
				</RelativeLayout>
				<TextView android:id="@+id/tv_telnum_title_old_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:gravity="center_vertical" android:text="@string/upomp_bypay_editinfo_oldtel"
					android:textSize="15sp" android:textColor="#6a7482"
					android:layout_marginLeft="10dip" android:layout_toRightOf="@+id/rl_iv_change_tel"
					android:layout_centerVertical="true" />

				<TextView android:id="@+id/tv_telnum_content_old_change_tel"
					android:singleLine="true" android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:numeric="integer"
					android:maxLength="11" android:textColor="#6a7482"
					android:layout_centerVertical="true" android:textSize="15sp"
					android:background="#00000000" android:layout_toRightOf="@+id/tv_telnum_title_old_change_tel" />

			</RelativeLayout>
			<ImageView android:id="@+id/iv_bot_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:background="@drawable/upomp_bypay_info_bot2"
				android:layout_below="@id/rl_telnum_old_change_tel"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip" />
			<RelativeLayout android:id="@+id/rl_telnum_new_change_tel"
				android:layout_width="fill_parent" android:layout_height="45dip"
				android:layout_below="@id/iv_bot_change_tel"
				android:layout_marginTop="8dip" android:background="@drawable/upomp_bypay_input_bg"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip">

				<TextView android:id="@+id/tv_telnum_new_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="@string/upomp_bypay_editinfo_newtel"
					android:layout_centerVertical="true" android:textColor="#6a7482"
					android:layout_marginLeft="10dip" android:gravity="center_vertical"
					android:textSize="15sp" />

				<EditText android:id="@+id/et_telnum_new_change_tel"
					android:singleLine="true" android:layout_width="fill_parent"
					android:layout_height="45dip" android:numeric="integer"
					android:maxLength="11" android:textColor="#6a7482"
					android:layout_toRightOf="@+id/tv_telnum_new_change_tel"
					android:hint="@string/upomp_bypay_editinfo_newtelhint"
					android:layout_centerVertical="true" android:textSize="15sp"
					android:background="#00000000" />

			</RelativeLayout>
			<RelativeLayout android:id="@+id/rl_validcode_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_below="@id/rl_telnum_new_change_tel"
				android:layout_marginTop="8dip" android:background="@drawable/upomp_bypay_input_bg"
				android:layout_marginLeft="10dip" android:layout_marginRight="10dip">

				<TextView android:id="@+id/tv_validcode_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="@string/upomp_bypay_firstshortmessagesecuritycode"
					android:textSize="15sp" android:textColor="#6a7482"
					android:layout_centerVertical="true" android:layout_marginLeft="10dip"
					android:gravity="center_vertical" />

				<EditText android:id="@+id/et_validcode_change_tel"
					android:singleLine="true" android:layout_width="fill_parent"
					android:layout_height="45dip" android:numeric="integer"
					android:textSize="15sp" android:maxLength="6" android:textColor="#6a7482"
					android:hint="@string/upomp_bypay_secondshortmessagesecuritycode"
					android:background="#00000000" android:layout_centerVertical="true"
					android:layout_toRightOf="@+id/tv_validcode_change_tel"
					android:layout_toLeftOf="@+id/btn_validcode_change_tel" />
				<Button android:id="@+id/btn_validcode_change_tel"
					android:layout_width="wrap_content" android:layout_height="45dip"
					android:layout_centerVertical="true"
					android:layout_alignParentRight="true" android:layout_marginLeft="5dip"
					android:background="@drawable/upomp_bypay_input_btn4" android:text="@string/upomp_bypay_get"
					android:textSize="15sp" android:textColor="#000000" />

			</RelativeLayout>


			<RelativeLayout android:id="@+id/rl_button_change_tel"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_below="@id/rl_validcode_change_tel"
				android:layout_marginTop="20dip">
				<Button android:id="@+id/btn_button_change_tel"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textSize="20sp" android:layout_centerHorizontal="true"
					android:background="@drawable/upomp_bypay_btn_enter2" android:text="@string/upomp_bypay_affirm"
					android:textColor="#FFFFFF" />
			</RelativeLayout>
		</RelativeLayout>
	</ScrollView>
	<com.unionpay.upomp.bypay.view.About
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_alignParentRight="true" android:paddingBottom="5dip" />
	<RelativeLayout android:id="@+id/main_dialog_change_tel"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_below="@id/rl_header_change_tel" android:layout_above="@+id/rl_bottom_change_tel" />
	<RelativeLayout android:id="@+id/rl_bottom_change_tel"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/upomp_bypay_bottom"
		android:layout_alignParentBottom="true">
	</RelativeLayout>
</RelativeLayout>
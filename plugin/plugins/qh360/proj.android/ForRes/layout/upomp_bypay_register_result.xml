<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/rl_register_result" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:background="@drawable/upomp_bypay_bg"
	android:layout_marginBottom="35dip" android:layout_marginLeft="15dip"
	android:layout_marginRight="15dip" android:layout_marginTop="35dip">
	<LinearLayout android:focusable="true"
		android:focusableInTouchMode="true" android:layout_width="0dip"
		android:layout_height="0dip" />
	<RelativeLayout android:id="@+id/rl_header_register_result"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/upomp_bypay_title_bg">
	</RelativeLayout>
	<ScrollView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_below="@id/rl_header_register_result"
		android:layout_above="@+id/rl_bottom_register_result">
		<RelativeLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content">

			<RelativeLayout android:id="@+id/rl_hint_register_result"
				android:layout_width="fill_parent" android:layout_height="wrap_content">
				<ImageView android:id="@+id/iv_hint_top_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_top2"
					android:layout_marginTop="20dip" android:layout_marginLeft="10dip"
					android:layout_marginRight="10dip" />

				<RelativeLayout android:id="@+id/rl_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_bg"
					android:layout_below="@id/iv_hint_top_register_result"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
					<RelativeLayout android:id="@+id/rl_iv_card_register_result"
						android:layout_width="40dip" android:layout_height="30dip"

						android:background="@drawable/upomp_bypay_info_left">
						<ImageView android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true" android:background="@drawable/upomp_bypay_info_icon1" />
					</RelativeLayout>
					<RelativeLayout android:id="@+id/rl_result_register_result"
						android:layout_width="wrap_content" android:layout_height="30dip"
						android:layout_toRightOf="@+id/rl_iv_card_register_result"
						android:layout_marginLeft="5dip" android:gravity="center_vertical">
						<TextView android:id="@+id/tv_result_content_register_result"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:text="@string/upomp_bypay_register_win"
							android:textColor="#ff0000" android:textSize="14sp" />
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout android:id="@+id/rl_info_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_below="@id/rl_register_result" android:background="@drawable/upomp_bypay_info_bg"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
					<ImageView android:layout_width="fill_parent"
						android:layout_height="1dip" android:background="#dbdee0"

						android:layout_marginRight="1dip" />
					<RelativeLayout android:id="@+id/rl_iv_info_register_result"
						android:layout_width="40dip" android:layout_height="40dip"

						android:background="@drawable/upomp_bypay_info_left">

					</RelativeLayout>
					<RelativeLayout android:id="@+id/rl_info_register_result"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_toRightOf="@+id/rl_iv_info_register_result">



						<RelativeLayout android:id="@+id/rl_orderinfo_info_register_result"
							android:layout_width="fill_parent" android:layout_height="40dip"
							android:gravity="center_vertical">

							<TextView android:id="@+id/tv_orderinfo_content_info_register_result"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:text="@string/upomp_bypay_register_win_hint"
								android:textColor="#6a7482" android:textSize="14sp" />
						</RelativeLayout>
					</RelativeLayout>
				</RelativeLayout>
				<ImageView android:id="@+id/iv_info_bot_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_bot2"
					android:layout_below="@id/rl_info_register_result"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip" />
			</RelativeLayout>


			<RelativeLayout android:id="@+id/rl_userinfo_register_result"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_below="@id/rl_hint_register_result"
				android:layout_marginTop="15dip">
				<ImageView android:id="@+id/iv_info_top_register_result1"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_top2"
					android:layout_marginLeft="10dip"
					android:layout_marginRight="10dip" />

				<RelativeLayout android:id="@+id/rl_into_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_bg"
					android:layout_below="@id/iv_info_top_register_result1"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip">
					<RelativeLayout android:id="@+id/rl_iv_card_register_result"
						android:layout_width="40dip" android:layout_height="60dip"

						android:background="@drawable/upomp_bypay_info_left">
						<ImageView android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true" android:background="@drawable/upomp_bypay_info_icon4" />
					</RelativeLayout>
					<RelativeLayout android:id="@+id/rl_result_register_result"
						android:layout_width="wrap_content" android:layout_height="60dip"
						android:layout_toRightOf="@+id/rl_iv_card_register_result"
						 android:gravity="center_vertical">
						<RelativeLayout android:id="@+id/rl_name_register_result"
							android:layout_width="fill_parent" android:layout_height="wrap_content">
							<TextView android:id="@+id/tv_name_title_register_result"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:text="@string/upomp_bypay_accountmain_username" android:textSize="14sp"
								android:textColor="#6a7482" android:layout_marginLeft="5dip" />
							<TextView android:id="@+id/tv_name_content_register_result"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:layout_toRightOf="@+id/tv_name_title_register_result"
								android:textColor="#6a7482" android:textSize="14sp" />
						</RelativeLayout>
						<ImageView android:layout_width="fill_parent" android:layout_below="@id/rl_name_register_result"
							android:layout_height="1dip" android:background="#dbdee0"
							android:layout_marginRight="1dip"  android:layout_marginTop="10dip" />
						<RelativeLayout android:id="@+id/rl_welcome_register_result"
							android:layout_width="fill_parent" android:layout_height="wrap_content"
							android:layout_below="@id/rl_name_register_result"
						 android:layout_marginTop="20dip">
							<TextView android:id="@+id/tv_welcome_title_register_result"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:text="@string/upomp_bypay_welcome" android:textSize="14sp"
								android:textColor="#6a7482" android:layout_marginLeft="5dip" />
							<TextView android:id="@+id/tv_welcome_content_register_result"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:layout_toRightOf="@+id/tv_welcome_title_register_result"
								android:textColor="#6a7482" android:textSize="14sp" />
						</RelativeLayout>


					</RelativeLayout>
				</RelativeLayout>

				<ImageView android:id="@+id/iv_info_bot_register_result"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:background="@drawable/upomp_bypay_info_bot2"
					android:layout_below="@id/rl_into_register_result"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip" />
			</RelativeLayout>
			
			<Button android:id="@+id/btn_exit_register_result"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:textSize="20sp" android:textColor="#ffffff"
				android:layout_marginTop="20dip"
				android:text="@string/upomp_bypay_register_bind_card"
				android:layout_below="@id/rl_userinfo_register_result"
				android:layout_centerHorizontal="true" android:background="@drawable/upomp_bypay_btn_esc2" />
		</RelativeLayout>
	</ScrollView>
<com.unionpay.upomp.bypay.view.About
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_alignParentRight="true" android:paddingBottom="5dip" />
	<RelativeLayout android:id="@+id/rl_bottom_register_result"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/upomp_bypay_bottom"
		android:layout_alignParentBottom="true" />
</RelativeLayout>
material 
{

	technique tech1
	{

		pass pas2
		{
			vertexShader = shaders/textured.vsh
			fragmentShader = shaders/textured.fsh
			defines = SKINNING_JOINT_COUNT 32
			u_diffuseColor = 1,1,1,1

			sampler u_diffuseTexture
			{
				path = 2.5D/fish/haigui/haigui.png
				mipmap = false
				wrapS = REPEAT
				wrapT = REPEAT
				minFilter = LINEAR
				magFilter = LINEAR
			}

			renderState 
			{
				blend = true
				srcBlend = SRC_ALPHA
				dstBlend = ONE_MINUS_SRC_ALPHA
				cullFace = true
				depthTest = true
				depthWrite = true
			}

		}

	}

}


material 
{

	technique 
	{

		pass 0
		{
			vertexShader = shaders/textured.vsh
			fragmentShader = shaders/textured.fsh
			defines = SKINNING_JOINT_COUNT 32
			u_diffuseColor = 1,1,1,1

			sampler u_diffuseTexture
			{
				path = 2.5D/girl/WuQi.png
				mipmap = true
				wrapS = CLAMP
				wrapT = CLAMP
				minFilter = NEAREST_MIPMAP_LINEAR
				magFilter = LINEAR
			}

			renderState 
			{
				cullFace = true
				depthTest = true
				depthWrite = true
			}

		}

	}

}


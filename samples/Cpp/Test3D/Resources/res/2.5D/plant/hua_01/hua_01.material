material test
{
	u_diffuseColor = 1.0, 1.0, 1.0, 1.0
	u_worldViewProjectionMatrix = WORLD_VIEW_PROJECTION_MATRIX
	u_inverseTransposeWorldMatrix = INVERSE_TRANSPOSE_WORLD_MATRIX
	u_worldMatrix = WORLD_MATRIX  
    u_matrixPalette = MATRIX_PALETTE
    
	// samplers
	sampler u_diffuseTexture
	{
		path = 2.5D/plant/hua_01/hua_01.png
		mipmap = true
		wrapS = REPEAT
		wrapT = REPEAT
		minFilter = LINEAR_MIPMAP_LINEAR
		magFilter = LINEAR
	}

	technique
    {
        pass 0
        {
            // shaders
            vertexShader = shaders/textured.vsh
            fragmentShader = shaders/textured.fsh
            
            // uniforms
			

            defines = SKINNING_JOINT_COUNT 32;MAXANIMLIGHT 1
            
            // render state
            renderState
            {
                cullFace = false
                depthTest = true

                depthWrite = true
                blend = true
                srcBlend = SRC_ALPHA
                dstBlend = ONE_MINUS_SRC_ALPHA
            }
        }
    }
	
	technique _castshadow
    {
        pass 0
        {
            // shaders
            vertexShader = shaders/textured.vsh
            fragmentShader = shaders/writedepth.fsh
            
            // uniforms
			

            defines = SKINNING_JOINT_COUNT 32
            
            // render state
            renderState
            {
                cullFace = false
                depthTest = true

                depthWrite = true
                blend = false
            }
        }
    }}
#ifndef _LIBCPP_CSTDBOOL
#define _LIBCPP_CSTDBOOL

/*
    cstdbool synopsis

Macros:

    __bool_true_false_are_defined

*/

#include <__config>

#if !defined(_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#pragma GCC system_header
#endif

#undef __bool_true_false_are_defined

        "project_replace_project_name": {
            "src_project_name": "HelloLua",
            "files": [
                "config.json",
                ".project"
            ]
        },
        "append_dir": [
            {
                "from": "cocos/scripting/lua-bindings/script",
                "to": "src/cocos",
                "exclude": []
            },
            {
                "from": "external/win32-specific/gles/prebuilt",
                "to": "runtime/win32",

               "proj.win32/PROJECT_NAME.vcxproj.user",
               "proj.win32/PROJECT_NAME.sln",
               "proj.ios_mac/PROJECT_NAME.xcodeproj",
               "proj.win8.1-universal/PROJECT_NAME.sln",
               "proj.win8.1-universal/App.Shared/PROJECT_NAME.Shared.vcxitems",
